{
  "name": "neurolens",
  "version": "1.0.0",
  "description": "Real-time neurological assessment platform with multi-modal AI analysis",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint": "bunx eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "bunx eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "bun tsc --noEmit",
    "format": "bunx prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "bunx prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "quality:check": "bun run type-check && bun run lint && bun run format:check",
    "quality:fix": "bun run format && bun run lint:fix && bun run type-check",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "analyze": "ANALYZE=true bun --bun next build",
    "perf:audit": "bunx lighthouse http://localhost:3000 --output=json --output=html --output-path=./performance-audit",
    "clean": "bun rimraf .next out dist node_modules/.cache",
    "prepare": "husky install",
    "turbo:build": "bunx turbo build",
    "turbo:dev": "bunx turbo dev",
    "turbo:lint": "bunx turbo lint",
    "turbo:test": "bunx turbo test",
    "turbo:clean": "bunx turbo clean && bun run clean",
    "install:clean": "bun run clean && bun install",
    "postinstall": "bun run type-check"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "^12.23.15",
    "lucide-react": "^0.541.0",
    "next": "15.5.3",
    "onnxruntime-web": "^1.22.0",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "22.10.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^16.3.0",
    "husky": "9.1.7",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rimraf": "6.0.1",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.2.20",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "neurology",
    "healthcare",
    "ai",
    "assessment",
    "pwa",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "NeuroLens Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neurolens/neurolens"
  },
  "bugs": {
    "url": "https://github.com/neurolens/neurolens/issues"
  },
  "homepage": "https://neurolens.com"
}
