{
  "name": "medilens",
  "version": "1.0.0",
  "description": "MediLens - AI-Powered Medical Analysis Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others-on-fail -n \"API,WEB\" -c \"green.bold,blue.bold\" \"bun run backend\" \"bun run frontend\"",
    "backend": "cd backend && .venv/Scripts/python.exe -m uvicorn app.main:app --reload --reload-dir app --port 8000",
    "backend:prod": "cd backend && .venv/Scripts/python.exe -m uvicorn app.main:app --port 8000 --workers 4",
    "frontend": "cd frontend && bun run dev",
    "frontend:build": "cd frontend && bun run build",
    "frontend:preview": "cd frontend && bun run preview",
    "install:backend": "cd backend && python -m venv .venv && .venv/Scripts/pip.exe install -r requirements.txt",
    "install:frontend": "cd frontend && bun install",
    "install:all": "bun run install:frontend && bun run install:backend",
    "build": "cd frontend && bun run build",
    "build:analyze": "cd frontend && ANALYZE=true bun run build",
    "test:backend": "cd backend && .venv/Scripts/python.exe -m pytest -v",
    "test:frontend": "cd frontend && bun test",
    "lint:frontend": "cd frontend && bun run lint",
    "health": "curl -s http://localhost:8000/health && echo '' && curl -s http://localhost:3000/api/health",
    "start": "bun run dev"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/steeltroops-ai/MediLens"
  },
  "author": "SteelTroops AI",
  "license": "MIT"
}
