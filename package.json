{
  "name": "neurolens-x",
  "version": "1.0.0",
  "description": "Multi-modal neurological risk assessment platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@tanstack/react-query": "5.51.11",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "framer-motion": "11.3.8",
    "html2canvas": "1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "0.408.0",
    "next": "^15.4.6",
    "next-pwa": "5.6.0",
    "postcss": "8.4.39",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.1",
    "react-hot-toast": "2.4.1",
    "recharts": "2.12.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.6",
    "zod": "3.23.8",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.5",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.34.4",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-axe": "8.0.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "5.0.8",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "neurology",
    "healthcare",
    "ai",
    "assessment",
    "pwa",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "NeuroLens-X Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neurolens-x/neurolens-x"
  },
  "bugs": {
    "url": "https://github.com/neurolens-x/neurolens-x/issues"
  },
  "homepage": "https://neurolens-x.com"
}
